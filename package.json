{
  "name": "lambda-streaming",
  "version": "1.0.0",
  "description": "Testing response streaming in AWS Lambda",
  "license": "MIT",
  "author": "Hendrik Liebau <mail@hendrik-liebau.de>",
  "type": "module",
  "scripts": {
    "compile:check": "tsc",
    "predeploy": "cdk bootstrap --app \"node create-stack.js\"",
    "deploy": "cdk deploy --app \"node create-stack.js\" --all --no-asset-metadata",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint .",
    "prepare": "node onecfg.js",
    "start": "node index.js",
    "test": "aws lambda invoke --function-name streaming-test --invocation-type Event --cli-binary-format raw-in-base64-out --payload '{\"requestId\":\"test-id\"}' /dev/null"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.264.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@aws-sdk/client-lambda": "^3.259.0",
    "@onecfg/standard": "^8.0.0",
    "@types/aws-lambda": "^8.10.110",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "aws-cdk": "^2.62.1",
    "aws-cdk-lib": "^2.62.1",
    "cdk-ec2-key-pair": "^3.3.1",
    "esbuild": "^0.17.5",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-markdown": "^3.0.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4"
  }
}
